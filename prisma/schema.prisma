// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model SpotifyAuthToken {
  // This is session dependant
  userCookie String @id
  
  refreshToken String 
  accessToken String
  expiresAt DateTime

  updatedAt DateTime @updatedAt
}